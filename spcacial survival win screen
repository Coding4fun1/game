<script>
function start(){
sjs.open();
var image = new sjs.Image("https://media.giphy.com/media/IjmMzurYulKEw/giphy.gif");
image.setSize(500, 400); 

var win_text = new sjs.Text("You Win!",28,"blue");
win_text.centerH(); 

var startBtn = new sjs.Button("Restart", function(){
window.location = "spacial_menu_screen.html";
});

startBtn.center(); 

}
</script>
